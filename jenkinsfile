pipeline {
  agent any

  environment {
    AWS_ACCESS_KEY_ID     = credentials('AWS_ACCESS_KEY_ID')
    AWS_SECRET_ACCESS_KEY = credentials('AWS_SECRET_ACCESS_KEY')
    AWS_DEFAULT_REGION    = 'us-east-1'
  }

  stages {
    stage('Install AWS CLI') {
      steps {
        sh 'pip install --upgrade awscli'
      }
    }

    stage('Upload Raw CSV to S3') {
      steps {
        sh 'aws s3 cp data/raw/ s3://rawbuckett1/ --recursive'
      }
    }

    stage('Upload Processed CSV to S3') {
      steps {
        sh 'aws s3 cp data/processed/ s3://finalbuckett3/ --recursive'
      }
    }
  }
}